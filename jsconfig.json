{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
        "baseUrl": "./",
        "moduleResolution": "node",
        "paths": {
            "@/*": ["src/*"],
            "@cmps/*": ["src/components/*"],
            "@views/*": ["src/views/*"],
            "@store/*": ["src/store/*"],
            "@api/*": ["src/api/*"]
        },
        "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
    }
}
